                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT        COUNT(Matriculas.id) AS Cantidad, Especialidades.descripcion
FROM            Matriculas INNER JOIN
                         Legajos_Academicos ON Matriculas.Legajo_Academico_id = Legajos_Academicos.id INNER JOIN
                         Titulos ON Legajos_Academicos.Titulo_id = Titulos.id INNER JOIN
                         Especialidades ON Titulos.Especialidad_id = Especialidades.id INNER JOIN
                         Profesionales ON Matriculas.Profesional_dni = Profesionales.dni INNER JOIN
                         Historiales ON Profesionales.dni = Historiales.Profesional_dni
WHERE        (Historiales.fecha &gt;= @fecha_menor) AND (Historiales.fecha &lt;= @fecha_mayor)
GROUP BY Especialidades.descripcion</CommandText>
